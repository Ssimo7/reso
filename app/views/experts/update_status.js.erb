<%- locals = { match: @match } %>

var selectedAssistanceExpert = "<%= j render(partial: 'experts/match', locals: locals) %>";
document.getElementById('selected-assistance-expert-line-<%= @match.id %>-global').innerHTML = selectedAssistanceExpert;

var selectedAssistanceExpertOwner = "<%= j render(partial: 'experts/match_for_need_owner', locals: locals) %>";
document.getElementById('selected-assistance-expert-line-<%= @match.id %>-owner').innerHTML = selectedAssistanceExpertOwner;

var expertButtons = "<%= j render(partial: 'experts/expert_buttons', locals: locals) %>";
document.getElementById('expert-buttons-line-<%= @match.id %>').innerHTML = expertButtons;
