%table
  %thead
    %tr
      %th= t('activerecord.models.company.one')
      %th= t('activerecord.models.question.one')
      %th
  %tbody
    - matches.each do |match|
      - diagnosis_id = match.diagnosed_need.diagnosis.id
      - more_info_url = diagnosis_experts_url(diagnosis_id: diagnosis_id, access_token: access_token)
      %tr
        %td= match.diagnosed_need.diagnosis.visit.company_name
        %td= match.diagnosed_need.question
        %td= link_to t('.show_information'), more_info_url
