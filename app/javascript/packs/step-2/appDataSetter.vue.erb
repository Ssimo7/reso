
<template></template>

<script>
import { mapMutations } from 'vuex'
import * as types from './store/mutationTypes'
import ErrorHandler from '../common/errorHandler'

export default {
  name: 'app-data-setter',
  props: [
    'diagnosisId',
    'diagnosisContent'
  ],
  created: function () {
    this.setDiagnosisId(this.diagnosisId)
    this.setDiagnosisContent(this.diagnosisContent)

    this.$store.dispatch('getDiagnosisContentValue').catch(ErrorHandler.report)
    this.$store.dispatch('getDiagnosedNeeds').catch(ErrorHandler.report)
  },
  methods: {
    ...mapMutations({
      setDiagnosisId: types.DIAGNOSIS_ID,
      setDiagnosisContent: types.DIAGNOSIS_CONTENT
    })
  }
}
</script>

