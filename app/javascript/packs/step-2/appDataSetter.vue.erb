<template></template>

<script>
    import { mapMutations } from 'vuex'
    import * as types from './store/mutationTypes'

    export default {
        name: 'app-data-setter',
        props: [
            'diagnosisId',
            'diagnosisContent'
            ],
        data: function () {
            return {
            }
        },
        created: function() {
            this.setDiagnosisId(this.diagnosisId);
            this.setDiagnosisContent(this.diagnosisContent);
        },
        methods: {
            ...mapMutations({
                setDiagnosisId: types.DIAGNOSTIC_ID,
                setDiagnosisContent: types.DIAGNOSTIC_CONTENT
            })
        }
    }
</script>

