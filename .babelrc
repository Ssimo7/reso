{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "ie": 10,
        "browsers": ["last 2 versions"],
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    ["transform-class-properties", { "spec": true }],
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
